sudo mkdir /docker
sudo cd /docker
sudo cp /var/lib/jenkins/workspace/package/target/addressbook.war .
touch Dockerfile
cat <<EOT>> Dockerfile
From tomcat
ADD addressbook.war  /usr/local/tomcat/webapps
CMD "catalina.sh" "run"
EXPOSE 8080
EOT
sudo docker build -t raghav18/raghav_deploy18:$BUILD_NUMBER .
sudo docker run -itd --name deploy-$BUILD_NUMBER -p 8080 raghav18/raghav_deploy18:$BUILD_NUMBER
